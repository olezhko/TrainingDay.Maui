            <array>
                <string>CrashReporting</string>
                <string>DeviceIdentifiers</string>
            </array>
            <key>Description</key>
            <string>Used for diagnostics and error reporting.</string>
        </dict>
        <dict>
            <key>SDKName</key>
            <string>GoogleDataTransport</string>
            <key>SDKVersion</key>
            <string>X.X.X</string>
            <key>DataCollections</key>
            <array>
                <string>UsageData</string>
            </array>
            <key>Description</key>
            <string>Handles transport of analytics data to Google services.</string>
        </dict>
    </array>
</dict>
</plist>
